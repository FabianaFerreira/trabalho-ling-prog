# the compiler
CC = g++
CFLAGS  = -Wall

OBJFILES =  cleanLyricsFile.o
TARGET = cleanLyricsFile.exe

all: $(TARGET)

$(TARGET): $(OBJFILES)
	$(CC) $(CFLAGS) -o $(TARGET) $(OBJFILES)

clean:
	rm -f $(OBJFILES) $(TARGET) *~
